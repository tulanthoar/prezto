# python -c "import time, datetime;then=datetime.datetime.now()+datetime.timedelta(days=3); print(time.mktime(then.timetuple())*1e3+then.microsecond/1e3)"
export T1=$(date +%S%N)
export ENHANCD_FILTER=fzf
export ENHANCD_COMMAND="cd"
ZPLUGINS=( 'willghatch/zsh-cdr' 'MikeDacre/cdbk' 'andrewferrier/fzf-z' 'psprint/history-search-multi-word' 'AlexisBRENON/oh-my-zsh-reminder' 'willghatch/zsh-saneopt' 'chrissicool/zsh-256color' 'Tarrasch/zsh-bd' 'Tarrasch/zsh-colors' 'RobSis/zsh-completion-generator' 'willghatch/zsh-snippets' 'uvaes/fzf-marks' 'bric3/nice-exit-code' 'djui/alias-tips' 'psprint/zsh-editing-workbench')
setopt CLOBBER
source "${ZDOTD}/antibody/ini.zsh"
antibody_zsh bundle <<< $(printf '%s\n' ${ZPLUGINS[@]})
source "${ZPREZD}/init.zsh"
source "${HOME}/.fzf/fzf.zsh"
source "${ZRCD}/directory-history.plugin.zsh"
source "${ZRCD}/myfunctions.zsh"
source "${ZRCD}/myaliases.zsh"
source "${ZRCD}/mysnippets.zsh"
source "${HOME}/.enhancd/init.sh"
# source "$ENHANCD_ROOT/src/utils.sh"
# source "$ENHANCD_ROOT/src/enhancd.sh"
# autoload -Uz chpwd_recent_dirs cdr add-zsh-hook # cdr
# add-zsh-hook chpwd chpwd_recent_dirs
# zstyle ':chpwd:*' recent-dirs-file "${XDG_CACHE_HOME:-$HOME/.cache}/zsh/chpwd-recent-dirs"
eval infocmp ${TERM} | sed 's/kbs=^[hH]/kbs=\\177/' | tic -
stty -ixon
RPROMPT='$(nice_exit_code)'
bind_keys
 # Bind up/down arrow keys to navigate through your history
bindkey -M viins "^B" directory-history-search-backward
bindkey -M viins "^F" directory-history-search-forward
zle -N history-search-multi-word
bindkey -M viins "^R" history-search-multi-word
export T2=$(date +%S%N)
perl -e 'my $num = int(($ENV{"T2"}-$ENV{"T1"})/1_000_000); print $num'
